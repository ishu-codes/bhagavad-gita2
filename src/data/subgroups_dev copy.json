[
  {
    "group": "1-1",
    "verses": [
      {
        "code": "01-01",
        "phrases": [
          "धृतराष्ट्र उवाच-",
          "धर्मक्षेत्रे कुरुक्षेत्रे समवेता युयुत्सवः",
          "मामकाः पाण्डवाश्चैव किमकुर्वत सञ्जय"
        ]
      }
    ]
  },
  {
    "group": "1-2",
    "verses": [
      {
        "code": "01-02",
        "phrases": [
          "सञ्जय उवाच-",
          "दृष्ट्वा तु पाण्डवानीकं व्यूढं दुर्योधनस्तदा",
          "आचार्यमुपसङ्गम्य राजा वचनमब्रवीत्"
        ]
      }
    ]
  },
  {
    "group": "1-3",
    "verses": [
      {
        "code": "01-03",
        "phrases": [
          "पश्यैतां पाण्डुपुत्राणामाचार्य महतीं चमूम्",
          "व्यूढां द्रुपदपुत्रेण तव शिष्येण धीमता"
        ]
      }
    ]
  },
  {
    "group": "1-4,5,6",
    "verses": [
      {
        "code": "01-04",
        "phrases": [
          "अत्र शूरा महेष्वासा भीमार्जुनसमा युधि",
          "युयुधानो विराटश्च द्रुपदश्च महारथः"
        ]
      },
      {
        "code": "01-05",
        "phrases": [
          "धृष्टकेतुश्चेकितानः काशिराजश्च वीर्यवान्",
          "पुरुजित्कुन्तिभोजश्च शैब्यश्च नरपुङ्गवः"
        ]
      },
      {
        "code": "01-06",
        "phrases": [
          "युधामन्युश्च विक्रान्त उत्तमौजाश्च वीर्यवान्",
          "सौभद्रो द्रौपदेयाश्च सर्व एव महारथाः"
        ]
      }
    ]
  },
  {
    "group": "1-7",
    "verses": [
      {
        "code": "01-07",
        "phrases": [
          "अस्माकं तु विशिष्टा ये तान्निबोध द्विजोत्तम",
          "नायका मम सैन्यस्य संज्ञार्थं तान्ब्रवीमि ते"
        ]
      }
    ]
  },
  {
    "group": "1-8",
    "verses": [
      {
        "code": "01-08",
        "phrases": [
          "भवान्भीष्मश्च कर्णश्च कृपश्च समितिञ्जयः",
          "अश्वत्थामा विकर्णश्च सौमदत्तिस्तथैव च"
        ]
      }
    ]
  },
  {
    "id": "01-09-1",
    "title": "अन्ये च बहवः शूरा मदर्थे त्यक्तजीविताः",
    "group": "1-9"
  },
  {
    "id": "01-09-2",
    "title": "नानाशस्त्रप्रहरणाः सर्वे युद्धविशारदाः",
    "group": "1-9"
  },
  {
    "id": "01-10-1",
    "title": "अपर्याप्तं तदस्माकं बलं भीष्माभिरक्षितम्",
    "group": "1-10"
  },
  {
    "id": "01-10-2",
    "title": "पर्याप्तं त्विदमेतेषां बलं भीमाभिरक्षितम्",
    "group": "1-10"
  },
  {
    "id": "01-11-1",
    "title": "अयनेषु च सर्वेषु यथाभागमवस्थिताः",
    "group": "1-11"
  },
  {
    "id": "01-11-2",
    "title": "भीष्ममेवाभिरक्षन्तु भवन्तः सर्व एव हि",
    "group": "1-11"
  },
  {
    "id": "01-12-1",
    "title": "तस्य सञ्जनयन्हर्षं कुरुवृद्धः पितामहः",
    "group": "1-12"
  },
  {
    "id": "01-12-2",
    "title": "सिंहनादं विनद्योच्चैः शङ्ख दध्मौ प्रतापवान्",
    "group": "1-12"
  },
  {
    "id": "01-13-1",
    "title": "ततः शङ्खाश्च भेर्यश्च पणवानकगोमुखाः",
    "group": "1-13"
  },
  {
    "id": "01-13-2",
    "title": "सहसैवाभ्यहन्यन्त स शब्दस्तुमुलोऽभवत्",
    "group": "1-13"
  },
  {
    "id": "01-14-1",
    "title": "ततः श्वेतैर्हयैर्युक्ते महति स्यन्दने स्थितौ",
    "group": "1-14"
  },
  {
    "id": "01-14-2",
    "title": "माधवः पाण्डवश्चैव दिव्यौ शङ्खौ प्रदध्मतुः",
    "group": "1-14"
  },
  {
    "id": "01-15-1",
    "title": "पाञ्चजन्यं हृषीकेशो देवदत्तं धनञ्जयः",
    "group": "1-15"
  },
  {
    "id": "01-15-2",
    "title": "पौण्ड्रं दध्मौ महाशङ्ख भीमकर्मा वृकोदरः",
    "group": "1-15"
  },
  {
    "id": "01-16-1",
    "title": "अनन्तविजयं राजा कुन्तीपुत्रो युधिष्ठिरः",
    "group": "1-16,17,18"
  },
  {
    "id": "01-16-2",
    "title": "नकुलः सहदेवश्च सुघोषमणिपुष्पकौ",
    "group": "1-16,17,18"
  },
  {
    "id": "01-17-1",
    "title": "काश्यश्च परमेष्वासः शिखण्डी च महारथः",
    "group": "1-16,17,18"
  },
  {
    "id": "01-17-2",
    "title": "धृष्टद्युम्नो विराटश्च सात्यकिश्चापराजितः",
    "group": "1-16,17,18"
  },
  {
    "id": "01-18-1",
    "title": "द्रुपदो द्रौपदेयाश्च सर्वशः पृथिवीपते",
    "group": "1-16,17,18"
  },
  {
    "id": "01-18-2",
    "title": "सौभद्रश्च महाबाहुः शङ्खान्दध्मुः पृथक् पृथक्",
    "group": "1-16,17,18"
  },
  {
    "id": "01-19-1",
    "title": "स घोषो धार्तराष्ट्राणां हृदयानि व्यदारयत्",
    "group": "1-19"
  },
  {
    "id": "01-19-2",
    "title": "नभश्च पृथिवीं चैव तुमुलोऽभ्यनुनादयन्",
    "group": "1-19"
  },
  {
    "id": "01-20-1",
    "title": "अथ व्यवस्थितान्दृष्ट्वा धार्तराष्ट्रान् कपिध्वजः",
    "group": "1-20,21(1)"
  },
  {
    "id": "01-20-2",
    "title": "प्रवृत्ते शस्त्रसम्पाते धनुरुद्यम्य पाण्डवः",
    "group": "1-20,21(1)"
  },
  {
    "id": "01-21-1",
    "title": "हृषीकेशं तदा वाक्यमिदमाह महीपते",
    "group": "1-20,21(1)"
  },
  {
    "id": "01-21-2",
    "title": "अर्जुनः उवाच- सेनयोरुभयोर्मध्ये रथं स्थापय मेऽच्युत",
    "group": "1-21(2),22"
  },
  {
    "id": "01-22-1",
    "title": "यावदेतानिरीक्षेऽहं योद्धकामानवस्थितान्",
    "group": "1-21(2),22"
  },
  {
    "id": "01-22-2",
    "title": "कैर्मया सह योद्धव्यमस्मिन् रणसमुद्यमे",
    "group": "1-21(2),22"
  },
  {
    "id": "01-23-1",
    "title": "योत्स्यमानानवेक्षेऽहं य एतेऽत्र समागताः",
    "group": "1-23"
  },
  {
    "id": "01-23-2",
    "title": "धार्तराष्ट्रस्य दुर्बुद्धेयुद्धे प्रियचिकीर्षवः",
    "group": "1-23"
  },
  {
    "id": "01-24-0",
    "title": "सञ्जय उवाच-",
    "group": "1-24"
  },
  {
    "id": "01-24-1",
    "title": "एवमुक्तो हृषीकेशो गुडाकेशेन भारत",
    "group": "1-24"
  },
  {
    "id": "01-24-2",
    "title": "सेनयोरुभयोर्मध्ये स्थापयित्वा रथोत्तमम्",
    "group": "1-24"
  },
  {
    "id": "01-25-1",
    "title": "भीष्मद्रोणप्रमुखतः सर्वेषां च महीक्षिताम्",
    "group": "1-25"
  },
  {
    "id": "01-25-2",
    "title": "उवाच पार्थ पश्यैतान्समवेतान्कुरूनिति",
    "group": "1-25"
  },
  {
    "id": "01-26-1",
    "title": "तत्रापश्यत्स्थितान् पार्थः पितृनथ पितामहान्",
